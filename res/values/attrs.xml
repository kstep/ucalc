<?xml version="1.0" encoding="utf-8" ?>
<resources>
    <attr name="keypad_mode_all" format="integer" />
	<attr name="keypad_mode_any" format="integer" />
    <attr name="keypad_mode_mask" format="integer" />

    <attr name="units_category">
        <enum name="time" value="0"></enum>
        <enum name="dist" value="1"></enum>
        <enum name="vol" value="2"></enum>
        <enum name="weight" value="3"></enum>
        <enum name="elec" value="4"></enum>
        <enum name="misc" value="5"></enum>
    </attr>

    <declare-styleable name="TableView">
        <attr name="numRows" format="integer" />
    </declare-styleable>

    <declare-styleable name="UButton">
		<attr name="keypad_mode_all" />
		<attr name="keypad_mode_any" />
		<attr name="keypad_mode_mask" />
    </declare-styleable>

    <declare-styleable name="UToggleButton">
        <attr name="keypad_mode_all" />
		<attr name="keypad_mode_any" />
		<attr name="keypad_mode_mask" />
        <attr name="radio_group" format="string" />
    </declare-styleable>

    <declare-styleable name="IntegerPreference">
        <attr name="minValue" format="integer" />
        <attr name="maxValue" format="integer" />
    </declare-styleable>
</resources>
